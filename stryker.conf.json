{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "mutate": ["src/services/*.ts"],
  "thresholds": {
    "low": 100,
    "high": 100
  },
  "ignoreStatic": false,
  "warnings": {
    "slow": false
  },
  "commandRunner": {
    "command": "runContracts"
  },
  "buildCommand": "tsc",
  "reporters": ["progress", "clear-text", "html", "json"],
  "concurrency": 2,
  "coverageAnalysis": "perTest"
}
